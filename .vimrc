set showmode
set tabstop=2
set shiftwidth=2
set expandtab
set hlsearch
set smartcase
set ignorecase
set number
set showmatch
filetype indent plugin on
set nocompatible
set wildmenu
set autoindent
set nostartofline
set ruler
set laststatus=2
set paste
let g:python_recommended_style=0

syntax enable
set background=dark
colorscheme deus 
